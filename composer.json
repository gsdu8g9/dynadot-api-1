{
  "name": "level23/dynadot-api",
  "type": "library",
  "description": "Implementation of the Dynadot API",
  "keywords": [
    "dynadot"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Teye Heimans",
      "role": "Developer",
      "email": "teye@level23.nl"
    },
    {
      "name": "Niek Bergman",
      "role": "Developer",
      "email": "niek@level23.nl"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "5.5.9"
    }
  },
  "require-dev": {
    "phpmd/phpmd": "@stable",
    "monolog/monolog": "^1.21",
    "sebastian/phpcpd": "*",
    "squizlabs/php_codesniffer": "2.*",
    "phpdocumentor/phpdocumentor": "2.*",
    "phpunit/phpunit": "^4.8|^5.5",
    "phploc/phploc": "*",
    "pdepend/pdepend": "2.2.4",
    "mayflower/php-codebrowser": "~1.1",
    "theseer/phpdox": "0.9.0",
    "johnkary/phpunit-speedtrap": "^1.0",
    "guzzlehttp/guzzle": "6.*",
    "nikic/php-parser": "3.0.0beta1"
  },
  "require": {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": ">=6.0",
    "psr/log": "^1.0",
    "sabre/xml": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Level23\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Level23\\Tests\\": "tests/"
    }
  }
}
